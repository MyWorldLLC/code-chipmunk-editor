{
    "name": "chipmunk",
    "displayName": "Chipmunk",
    "description": "VS Code syntax highlighting & code folding support for the Chipmunk programming language.",
    "publisher": "MyWorldLLC",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "chipmunk",
            "aliases": ["Chipmunk", "chipmunk"],
            "extensions": [".chp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "chipmunk",
            "scopeName": "source.chipmunk",
            "path": "./syntaxes/chipmunk.tmLanguage.json"
        }]
    }
}